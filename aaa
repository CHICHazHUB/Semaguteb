--#region Diamonds Log
function DiamondsLog()
    _G.diamonds_earn = {}
    _G.diamonds_average = 0

    local function FormatNumber(n)
        n = tostring(n)
        return n:reverse():gsub("%d%d%d", "%1,"):reverse():gsub("^,", "")
    end

    task.spawn(function()
        while task.wait() do
            local diamonds_start = game:GetService("Players").LocalPlayer.leaderstats["\240\159\146\142 Diamonds"].Value
            task.wait(300)
            local diamonds_end = game:GetService("Players").LocalPlayer.leaderstats["\240\159\146\142 Diamonds"].Value
            local diamonds_total = diamonds_end - diamonds_start

            table.insert(_G.diamonds_earn, diamonds_total)
            local sum = 0
            for _, value in ipairs(_G.diamonds_earn) do
                sum = sum + value
            end
            _G.diamonds_average = sum / #_G.diamonds_earn

            print(("+%s Diamonds | Avg: %s | Day: %s"):format(FormatNumber(diamonds_total),
                FormatNumber(math.round(_G.diamonds_average)), FormatNumber(math.round(_G.diamonds_average) * 288)))
        end
    end)
end

DiamondsLog()
--#endregion

--#region f8 Auto เปิดไข่ล่าสุด & วาร์ปกลับที่เดิมให้
_G.auto_hatch_eggs = false

game:GetService("UserInputService").InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.F8 then
        _G.auto_hatch_eggs = not _G.auto_hatch_eggs
        if _G.auto_hatch_eggs then
            AutoHatchEggs()
        end
    end
end)

function AutoHatchEggs()
    task.spawn(function()
        local oldPosition = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame
        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(164.104996, 13.5860004,
            -204.813995)
        task.wait(1)

        repeat task.wait() until #workspace.__THINGS.Eggs.Main:GetChildren() > 0
        local eggs = {}
        for _, v in pairs(workspace.__THINGS.Eggs.Main:GetChildren()) do
            table.insert(eggs, v.Name)
        end
        function Sort(a, b)
            local numA = tonumber(string.match(a, "(%d+)"))
            local numB = tonumber(string.match(b, "(%d+)"))
            return numA < numB
        end

        table.sort(eggs, Sort)

        local latestEgg
        for _, v in pairs(eggs) do
            if workspace.__THINGS.Eggs.Main[v].PriceHUD:FindFirstChild("PriceHUD") then
                if not workspace.__THINGS.Eggs.Main[v].PriceHUD.PriceHUD:FindFirstChild("PriceHUDLocked") then
                    latestEgg = v
                end
            end
            if workspace.__THINGS.Eggs.Main[v].PriceHUD:FindFirstChild("PriceHUDAvailable") then
                if workspace.__THINGS.Eggs.Main[v].PriceHUD.PriceHUDAvailable.TextLabel.Text == "Unlock!" then
                    latestEgg = v
                end
            end
        end

        local position = workspace.__THINGS.Eggs.Main[latestEgg].PriceHUD.CFrame.Position
        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(position.X + 5, position.Y,
            position.Z)
        task.wait(1)

        repeat task.wait() until workspace.__THINGS.Eggs.Main[latestEgg]:FindFirstChild("EggInfo")
        local data = require(game:GetService("ReplicatedStorage").Library).Save.Get()
        local eggName = workspace.__THINGS.Eggs.Main[latestEgg].EggInfo.Frame.Title.Text
        -- local times = data.EggHatchCount >= Amount and 1 or math.ceil(Amount /  data.EggHatchCount)

        while task.wait() do
            if not _G.auto_hatch_eggs then break end
            local args = {
                [1] = eggName,
                [2] = data.EggHatchCount
            }
            game:GetService("ReplicatedStorage").Network.Eggs_RequestPurchase:InvokeServer(unpack(args))
            task.wait(0.5)
        end
        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = oldPosition
    end)
end

